    <script>
      const $pages = document.querySelectorAll("[data-page-tab]");
      const $tab = document.querySelector(".page-tab");

      $pages.forEach((page, index) =>{
        if(index === 0){
        } else {
          page.setAttribute("hidden", true);
        }
      })


      const pageFunc = function () { $tab.addEventListener("click", (e)=>{
        const clicked = e.target.closest("li");
        e.preventDefault();
        if(!clicked) return



        $tab.querySelectorAll("li").forEach(tab => { tab.classList.remove('activated') });
        clicked.classList.add('activated');

        $tab.style.setProperty("--_left", clicked.offsetLeft+'px');


        const currentPageId = e.target.getAttribute( 'href' );
        const currentPage = document.querySelector(currentPageId)

        $pages.forEach(page =>{page.setAttribute("hidden", true)});
        currentPage.removeAttribute("hidden")

        idScroll();
      });

      }

      function idScroll() {
        const currentPageSelectId = document.querySelector(".activated").querySelector("a").getAttribute("href");
        const $currentPageSelect = document.querySelector(currentPageSelectId);
        
        const $tabArea = $currentPageSelect.querySelector(".css-team-tab");
        const $contentInTab = $tabArea.querySelectorAll("a");

        console.log($currentPageSelect)

        $tabArea.addEventListener("click", (e)=>{
          const clickedTab = e.target.closest("a");

          e.preventDefault();
          if(!clickedTab) return

          const selectedTabId =clickedTab.getAttribute("href");
          console.log(selectedTabId);
          const scrollPosition= $currentPageSelect.querySelector(selectedTabId).offsetTop;
          console.log(scrollPosition);

          window.scrollTo({ top: scrollPosition, behavior: 'smooth' });
        });
      }

      pageFunc();
      idScroll();
      </script>
    </div>
  </main>

