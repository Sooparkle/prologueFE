<script>
  const $tablePopup = document.querySelector("table");
  const $pupup =document.querySelector(".popup");

console.log($popup);
  const pupopOpen = () =>{
  $tablePopup.addEventListener("click", (e)=>{
    const clickedSVG = e.target.closest("svg");


    console.log(clickedSVG);
    if(!clickedSVG) return
    $pupup.classList.add("activated");
    
  })
  $popup.querySelector("button").addEventListener("cilick",()=>{
    $pupup.classList.remove("activated");
  })


}

  
</script>

targetTag.setAttribute("class", "popup")
