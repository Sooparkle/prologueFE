          <script>
            const searchDetail = () =>{
              const detailSearch = document.querySelector(".button-wrap");
              const teamSelectBox = document.querySelector(".form-wrap");
              let isOpen = teamSelectBox.getAttribute("data-form") === "false";

              detailSearch.addEventListener("click", (e) => {
                e.target.closest("button");
                isOpen = !isOpen

                if (isOpen) {
                  teamSelectBox.setAttribute("data-form", "true");
                  teamSelectBox.style.height = "100px";
                  detailSearch.querySelector(".css-o82kp5").style.setProperty('transform', 'rotate(270deg)' );

                } else if (!isOpen){
                  teamSelectBox.setAttribute("data-form", "false");
                  teamSelectBox.style.height = "0";
                  detailSearch.querySelector(".css-o82kp5").style.setProperty('transform', 'rotate(90deg)' );

                }


              });
            }

          searchDetail()
          </script>



