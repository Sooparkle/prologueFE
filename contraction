        $(".m_emb").hover(function () {
            let idx = $(this).parent().index();
            let border_set = "5px solid " + color1[idx];
            $(this).css({ border: border_set });
            // console.log(border_set)
        }, function () {
            let border_set = "1px solid " + default_color;
