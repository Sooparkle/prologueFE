                $(dash).css({"background-color":"#000"});
                $(".logo_wrap span, .gnb > ul > li > a, .top_menu > ul > li > a").css({"color":"#000"});
                $(".logo_wrap_btn img").attr('src', "../images/arrow_d.png");
                $(".header").css({"height":"100%", "background":"rgba(0,0,0, 0.5)"});
                $(".search_wrap").css({"display":"block"});
                $(".gnb dl, .nav_bg, .logo_category").css({"display":"none"});
            } else if(active == 1){
                active = 0;
                $(".header").css({"height":"100%", "background":"none"});
