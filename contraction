          <script>
            const $content = document.querySelector(".table_wrap");
            const $popup = document.querySelector(".popup");
            let pupOpen = $popup.getAttribute("data-popup");
        
              $content.addEventListener("click", (e)=>{
                const clickedSVG = e.target.closest("svg");
                if(!clickedSVG) return
        
                const $name = document.querySelector("#name") ;
                $popup.setAttribute("data-popup", "true")
                $name.textContent = clickedSVG.parentElement.textContent;
        
              });
        
              $popup.querySelector("button").addEventListener("click",(e)=>{        
                $popup.setAttribute("data-popup", "false")
                
              });
      
          </script>

