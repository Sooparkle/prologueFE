  <script>
    const tabFunction = () =>{
      const content = document.querySelector(".content");
      const pages = document.querySelectorAll("[data-page-tab]");
      const tabContainer =document.querySelector(".page-tab");
      const tabList = tabContainer.querySelectorAll("li");
      const tabUl = tabContainer.querySelector("ul");

      tabContainer.addEventListener("click", (e)=>{
        const clickedTab = e.target.closest("li")

        e.preventDefault();
        if(!clickedTab) return;

        tabList.forEach(li =>{li.classList.remove("activated")});
        clickedTab.classList.add("activated");

        tabUl.style.setProperty("--_left", clickedTab.offsetLeft+"px");

        const activatedPageId = e.target.getAttribute("href");
        const activatedPage = document.querySelector(activatedPageId);


        pages.forEach((page, index) =>{
          page.setAttribute("hidden", true);
        });
        activatedPage.removeAttribute("hidden");


        const selectedTables = activatedPage.querySelectorAll("table");

          selectedTables.forEach(table =>{
            table.addEventListener("click", ()=>{
              const tableIndex = [...selectedTables]
              console.log(tableIndex);
              
            })



        })

      });
      };

      tabFunction()


  //   const tableFunction = () =>{
  //     const main = document.querySelector("main");
  //     const tabs = document.querySelectorAll("[data-tab]");
  //     const tables = document.querySelectorAll("[data-table]");

  //     main.addEventListener("click", (e)=>{
  //       const clicked = e.target
  //       e.preventDefault();


  //       tabs.forEach(tab => tab.setAttribute("data-tab", false));
  //       tab.setAttribute("data-tab", true);

  //       tables.forEach(table =>{table.setAttribute("data-table", false)});
  //       tables[index].setAttribute("data-table", true);

  //   });

  // }
  // tableFunction()
  </script>





